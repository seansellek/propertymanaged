%h1 Register as a Tenant

=form_for @tenant do |f|
  =f.label :name
  =f.text_field :name
  %br
  =f.label :email
  =f.email_field :email
  %br
  =f.label :password
  =f.password_field :password
  %br
  =f.label :password_confirmation
  =f.password_field :password_confirmation
  =f.hidden_field :token, value: @token
  =f.submit
